# Copy this file to `android/keystore.properties` and fill in the values
# to configure the release signing credentials. Keep the populated file
# out of version control.

# Absolute or relative path (from the project root) to your release keystore file
RELEASE_STORE_FILE=android/app/my-release-key.jks

# Alias name used when the keystore was generated
RELEASE_KEY_ALIAS=matrixchatRelease

# Passwords for the keystore and private key
RELEASE_STORE_PASSWORD=change-me
RELEASE_KEY_PASSWORD=change-me
